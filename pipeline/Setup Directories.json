					{
						"activity": "List Source Files",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"variableName": "DummyFile",
					"value": {
						"value": "@activity('List Source Files').output.childItems[0].name",
						"type": "Expression"
					}
				}
			},
